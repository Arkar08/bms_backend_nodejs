generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String  @id @default(uuid())
  email         String  @unique
  name          String
  password      String
  balance       Float   @default(0)
  isDeleted     Boolean @default(false)
  isDeactivated Boolean @default(false)
  stateCode     String
  townshipCode  String
}
